{
  "name": "vigour-ua",
  "version": "3.1.0",
  "description": "lightweight ua parser",
  "main": "index.js",
  "scripts": {
    "test": "(ducktape; NODE_ENV=test node test/index.js) | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "dev": "nodemon ./test | tap-difflet",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/ua.git"
  },
  "keywords": [
    "useragent",
    "browserify"
  ],
  "author": "Jim de Beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/ua/issues"
  },
  "homepage": "https://github.com/vigour-io/ua#readme",
  "contributors": [],
  "devDependencies": {
    "coveralls": "^2.11.9",
    "ducktape": "^1.0.0",
    "istanbul": "^0.4.2",
    "pre-commit": "^1.1.3",
    "tap-difflet": "0.4.0",
    "tape": "^4.5.1",
    "nodemon": "^1.11.0"
  }
}
